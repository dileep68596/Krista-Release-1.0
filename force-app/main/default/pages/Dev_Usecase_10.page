<apex:page standardController="Account">
    <apex:form >
        <apex:pageBlock title="inputText">
            <apex:pageBlockSection >
                <apex:inputText value="{!Account.Name}"/>
                <apex:inputText value="{!Account.Phone}"/>
                <apex:inputText value="{!Account.AnnualRevenue}"/>
                <apex:inputText value="{!Account.Industry}"/>
                <apex:inputText value="{!Account.SLAExpirationDate__c}"/>
            </apex:pageBlockSection>
        </apex:pageBlock>
        
        <apex:pageBlock title="inputField">
            <apex:pageBlockSection >
                <apex:inputField value="{!Account.Name}"/>
                <apex:inputField value="{!Account.Phone}"/>
                <apex:inputField value="{!Account.AnnualRevenue}"/>
                <apex:inputField value="{!Account.Industry}"/>
                <apex:inputField value="{!Account.SLAExpirationDate__c}"/>
            </apex:pageBlockSection>
        </apex:pageBlock>	
    </apex:form>
</apex:page>